  "devDependencies": {
    "@types/node": "^22.15.17",
    "autoprefixer": "^10.4.21",
    "postcss": "^8.5.3",
    "prettier": "^3.5.3",
    "tailwindcss": "^3.4.17",
    "typescript": "~5.8.3",
    "vite": "^6.3.5"
  },
  "dependencies": {
    "js-cookie": "^3.0.5",
    "typescript-cookie": "^1.0.6"
  }
}
