{
  "name": "mon-projet-front",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "build:css": "tailwindcss -i ./src/style/input.css -o ./dist/output.css",
    "dev:css": "tailwindcss -i ./src/style/input.css -o ./dist/output.css --watch",
    "start": "serve -s dist -l 8443"
